{
    "name": "lara-zeus/bolt",
    "description": "laravel package to create and manage forms",
    "keywords": [
        "laravel",
        "lara-zeus",
        "bolt",
        "bolt",
        "form",
        "forms",
        "generator",
        "maker"
    ],
    "homepage": "https://github.com/lara-zeus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "codeat3/blade-clarity-icons": "^1.7",
        "filament/notifications": "^2.0",
        "filament/spatie-laravel-translatable-plugin": "^2.0",
        "lara-zeus/core": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "orchestra/testbench": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Bolt\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Bolt\\BoltServiceProvider"
            ],
            "aliases": {
                "Bolt": "LaraZeus\\Bolt\\Facades\\Bolt"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ]
}
